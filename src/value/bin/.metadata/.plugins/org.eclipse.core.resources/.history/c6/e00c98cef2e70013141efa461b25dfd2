<c:when test="${ TossWinningId == matchDetails.innings[0].battingTeam.teamId }">
<c:set var="tossStatus" value="${ matchDetails.innings[0].battingTeam.teamName} won the toss and elected to bat first"/>
</c:when>
<c:otherwise>
